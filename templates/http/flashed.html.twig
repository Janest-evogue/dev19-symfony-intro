{% extends 'base.html.twig' %}

{% block body %}
    {# {{ dump(app.session.flashBag.all) }} #}

    {% for type, messages in app.session.flashBag.all %}
        <strong>{{ type }}</strong><br>
        {% for message in messages %}
            {{ message }}<br>
        {% endfor %}
    {% endfor %}

{% endblock %}
